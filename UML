@startuml
!theme carbon-gray


abstract class Usuario{
 - Nome: str
 - email: str
 - senha: str
 + autenticar(): bool
 + alterarSenha(nova_senha: str): bool
}

class Morador{
 - ListaPessoal: ListaPessoal 
 + adicionarProduto(produto: ProdutoBase, quantidade: int): None
 + removerProduto(produto: ProdutoBase): None
 + listarProdutos(): Dict[ProdutoBase, int]
}

class Administrador{}

Usuario <|-- Morador
Usuario <|-- Administrador


abstract class ProdutoBase{
 - nome: str
 - categoria: str
 - preco: float
}

class ProdutoGeral{}

class ProdutoPessoal{
 - pertence_a: Morador
}

ProdutoBase <|-- ProdutoGeral
ProdutoBase <|-- ProdutoPessoal

abstract class ListadeCompras{
 - lista_de_produtos: Dict[ProdutoBase, int]
 + adicionarProduto(produto: ProdutoBase, quantidade: int): None
 + removerProduto(produto: ProdutoBase): None
 + listarProdutos(): Dict[ProdutoBase, int]
}

class ListaGeral{}

class ListaPessoal{
 - morador: Morador
}

ListadeCompras <|-- ListaGeral
ListadeCompras <|-- ListaPessoal

class Residencia{
 - administrador: Administrador
 - moradores: List[Morador]
 - dispensa: Dispensa
 - lista_geral: ListaGeral
 - lista_pessoal: Dict[Morador, ListaPessoal]
 + adicionarMorador(morador: Morador): None
 + removerMorador(morador: Morador): None
}

class Dispensa{
 - estoque: Dict[ProdutoBase, int]
 + adicionarProduto(produto: ProdutoBase, quantidade: int): None
 + removerProduto(produto: ProdutoBase, quantidade: int): None
 + listarProdutos(): Dict[ProdutoBase, int]
}

Administrador <-- Residencia
Morador <-- Residencia
Dispensa <--- Residencia

@enduml
